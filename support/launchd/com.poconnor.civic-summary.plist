<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.poconnor.civic-summary</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/poconnor/go/bin/civic-summary</string>
        <string>process</string>
        <string>--all</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>10</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
        <key>HOME</key>
        <string>/Users/poconnor</string>
    </dict>

    <key>WorkingDirectory</key>
    <string>/Users/poconnor</string>

    <key>StandardOutPath</key>
    <string>/Users/poconnor/Library/Logs/civic-summary/civic-summary.stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/poconnor/Library/Logs/civic-summary/civic-summary.stderr.log</string>

    <key>KeepAlive</key>
    <false/>

    <key>RunAtLoad</key>
    <false/>
</dict>
</plist>
